{
  "name": "drewbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "cleanup": "lsof -t -i:1999 -i:5173 -i:5174 | xargs -r kill -9 || true",
    "dev": "concurrently \"pnpm start:client\" \"pnpm start:server\"",
    "start:client": "vite",
    "start:server": "partykit dev --live",
    "compress:dict": "gzip -c resources/dictionary.txt > public/dictionary.txt.gz",
    "build": "pnpm run compress:dict && vite build",
    "deploy": "pnpm run build && partykit deploy",
    "test": "pnpm run test:all",
    "test:all": "vitest run",
    "test:client": "vitest run --exclude test/party --exclude party",
    "test:server": "vitest run test/party party"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.154.14",
    "clsx": "^2.1.1",
    "partysocket": "^1.1.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-grid-layout": "^2.2.2",
    "react-resizable": "^3.1.3",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.14",
    "jsdom": "^28.1.0",
    "partykit": "^0.0.115",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
